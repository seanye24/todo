  {
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:jsx-a11y/recommended",
      "airbnb",
      "react-app",
      "plugin:react-hooks/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 11
    },
    "plugins": [
      "import",
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  }
